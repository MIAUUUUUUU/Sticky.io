@model List<MiauCore.IO.Models.Client>

@{
    ViewBag.Title = "Client - List";
}

<main role="main" class="col-md-6 col-md-offset-3">
    <h1>News</h1>

    <table class="table table-striped">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Telephone</th>
                <th>Email</th>
                <th>TaxId</th>
                <th>Rate</th>
                <th>MobilePhone</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var reward in Model)
            {
                <tr>
                    <td>@reward.Id</td>
                    <td>@reward.Name</td>
                    <td>@reward.Telephone</td>
                    <td>@reward.Email</td>
                    <td>@reward.TaxId</td>
                    <td>@reward.Rate</td>
                    <td>@reward.MobilePhone</td>
                </tr>
            }
        </tbody>
    </table>
</main>
