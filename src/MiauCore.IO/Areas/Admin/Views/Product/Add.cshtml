@model MiauCore.IO.Models.Product

@{
    ViewBag.Title = "Account - Register";
}

<main role="main" class="col-md-6 col-md-offset-3">
    <h1>Product - Register</h1>

    <form asp-area="Admin" asp-controller="Product" asp-action="Add" method="post">
        <div class="form-group">
            <label asp-for="Name"></label>
            <input id="title" asp-for="Name" class="form-control" type="text" />
        </div>
        <div class="form-group">
            <label asp-for="Type"></label>
            <input id="bannerimage" asp-for="Type" class="form-control" type="text" />
        </div>
        
        <input class="btn btn-primary" type="submit" value="Insert Product">
    </form>
</main>

